  "scripts": {
    "postinstall": "typings install",
    "start": "gulp start-server-production",
    "start-debug": "gulp start-server-debug",
    "test": "npm run snyk-test && npm run test-server && npm run test-client",
    "test-server": "gulp test-server",
    "test-client": "karma start",
    "test-client-watch": "karma start --auto-watch --no-single-run --browsers Chrome",
    "test-client-with-coverage": "karma start --reporters jasmine-diff,mocha,coverage  && remap-istanbul -i coverage/client/temp/coverage-final.json -t html -o coverage/client/final",
    "build-production": "webpack --config webpack.prod.js",
    "lint-client": "tslint ./src/client/**/*.ts",
    "docs": "typedoc ./src/client/main.ts --out doc --ignoreCompilerErrors --mode file",
    "snyk-test": "snyk test",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
